Content-Type: text/ecl
Entry-Name: .content

file-poosl-debug "nl.esi.poosl.examples/models-basic/HelloWorld/hello_world.poosl"
select-root-click-record
click-resume

wait 50
get-menu "Run/Terminate" | click

// HINT: we must explicit close the opened dialog, otherwise we can not select the console below.
get-window "Simulator has stopped" | get-button "OK" | click

try -times 10 -delay 50 -command {
	with [get-view Console] {
		// HINT: label output is slightly since eclipse 2020-06
		//get-label "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | assert-non-empty
		get-label | get-text | contains "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | verify-true
	}
} -finally {
	with [get-view Console] {
		// HINT: label output is slightly since eclipse 2020-06
		//get-label "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | assert-non-empty
		get-label | get-text | contains "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | verify-true
	}
	get-text-viewer | get-property text | contains terminated | verify-false
}
